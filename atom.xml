<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>Muammar El Khatib</title>
  <link href="http://muammar.me/atom.xml" rel="self"/>
  <link href="http://muammar.me/"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://muammar.me/</id>
  <author>
    <name>Muammar El Khatib</name>
    <email>muammar@muammar.me</email>
  </author>
  {% for post in site.posts limit:10 %}
  <entry>
    <title>{{ post.title }}</title>
    <link href="http://muammar.me{{ post.url }}"/>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>http://muammar.me{{ post.id }}</id>
    <content type="html">{{ post.content | xml_escape }}</content>
    <author>
      <name>Muammar El Khatib</name>
      <uri>http://muammar.me/about/</uri>
    </author>
  </entry>
  {% endfor %}
</feed>
